<% if (bannerHome.active) { %>
  <div id="carouselExampleControls" class="carousel slide carousel-fade d-none d-lg-block"
    style="height: calc(100vh - 63.99px);" data-bs-ride="carousel">

    <div class="carousel-inner h-100">
      <% bannerHome.images.forEach((file,index)=> { %>
        <div class="carousel-item <%= index === 0 ? " active" :null %>">
          <img class="animate-scale-img w-100" src="<%= /http/.test(file.img) ? file.img : " images/Banner/" + file.img %>"
          class="d-block w-100" alt="...">
        </div>
      <% }) %>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
      <span class="carousel-control-prev-icon w-50 w-lg-100" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
      <span class="carousel-control-next-icon w-50 w-lg-100" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>
  <% } %>